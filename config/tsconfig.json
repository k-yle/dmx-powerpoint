// this is the tsconfig used by the electron process
{
  "compilerOptions": {
    "outDir": "../dist/electron",
    "module": "commonjs",
    "target": "es2015",
    "moduleResolution": "node",
    "downlevelIteration": true
  },
  "include": ["../electron/**/*.ts"],
  "exclude": ["node_modules"]
}
